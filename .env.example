# =================================================================
# App SSO Environment Configuration Template
# =================================================================
# Copy this file to .env and customize the values for your environment

# =================================================================
# VERSION & DEPLOYMENT
# =================================================================
VERSION=latest
NODE_ENV=development

# =================================================================
# NETWORKING & HOSTS
# =================================================================
HOST=http://eservice.localhost
MOCKPASS_SERVER_HOST=http://mockpass.localhost

# =================================================================
# PORT CONFIGURATION
# =================================================================
WEB_HTTP_PORT=80
WEB_HTTPS_PORT=443
KC_PORT=8081
KC_DB_PORT=5432
ACEAS_API_PORT=3000
CPDS_API_PORT=3001
IDS_PORT=7000

# =================================================================
# KEYCLOAK CONFIGURATION
# =================================================================
# Admin credentials (change in production!)
KC_ADMIN_USER=admin
KC_ADMIN_PASSWORD=admin

# Hostname configuration
KC_HOSTNAME_URL=http://eservice.localhost
KC_HOSTNAME_ADMIN_URL=http://eservice.localhost

# Database connection
KC_DB_NAME=keycloak
KC_DB_USER=keycloak
KC_DB_PASSWORD=keycloak

# Database connection pool
KC_DB_POOL_INITIAL=5
KC_DB_POOL_MIN=5
KC_DB_POOL_MAX=20

# Logging
KC_LOG_LEVEL=info

# =================================================================
# OAUTH/OIDC CLIENTS
# =================================================================
# CPDS Client
CPDS_CLIENT_ID=cpds-spa
CPDS_CLIENT_SECRET=cpds-oidc-secret
CPDS_AUDIENCE=cpds-api

# ACEAS Client
ACEAS_AUDIENCE=aceas-api

# =================================================================
# MOCKPASS CONFIGURATION
# =================================================================
MOCKPASS_NRIC=S8979373D
SINGPASS_CLIENT_PROFILE=direct_pii_allowed
MYINFO_SECRET=your-client-secret
ENCRYPT_MYINFO=true

