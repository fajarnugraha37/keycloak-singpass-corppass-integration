<!-- Enhanced Console Section with 3D Effects -->
<section id="console" class="console-section scroll-3d-parallax scroll-3d-perspective">
    <div class="container">
        <div class="console-wrapper">
            <div class="console-header">
                <div class="console-title">
                    <svg class="console-icon" fill="currentColor" viewBox="0 0 20 20">
                        <path fill-rule="evenodd"
                            d="M3 4a1 1 0 011-1h12a1 1 0 011 1v2a1 1 0 01-1 1H4a1 1 0 01-1-1V4zm0 4a1 1 0 011-1h12a1 1 0 011 1v6a1 1 0 01-1 1H4a1 1 0 01-1-1V8zm2 2a1 1 0 000 2h.01a1 1 0 100-2H5zm3 0a1 1 0 000 2h3a1 1 0 100-2H8z"
                            clip-rule="evenodd"></path>
                    </svg>
                    <span>Console Output</span>
                    <div id="console-status" class="console-status">
                        <div class="status-indicator"></div>
                        <span>Ready</span>
                    </div>
                </div>
                <div class="console-actions">
                    <button id="clear-console" class="console-btn">
                        <svg fill="currentColor" viewBox="0 0 20 20">
                            <path fill-rule="evenodd"
                                d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z"
                                clip-rule="evenodd"></path>
                        </svg>
                        Clear
                    </button>
                    <button id="copy-console" class="console-btn">
                        <svg fill="currentColor" viewBox="0 0 20 20">
                            <path d="M8 3a1 1 0 011-1h2a1 1 0 110 2H9a1 1 0 01-1-1z" />
                            <path
                                d="M6 3a2 2 0 00-2 2v11a2 2 0 002 2h8a2 2 0 002-2V5a2 2 0 00-2-2 3 3 0 01-3 3H9a3 3 0 01-3-3z" />
                        </svg>
                        Copy
                    </button>
                </div>
            </div>
            <div class="console-content">
                <pre id="out" class="console-output">
<span class="console-welcome">ðŸ‘¥ Welcome to SDPC Personnel Management Platform</span>
<span class="console-info">ðŸš€ Government SSO ready for interaction...</span>
<span class="console-timestamp">[${new Date().toLocaleTimeString()}] System initialized</span>
              </pre>
            </div>
        </div>
    </div>
</section>